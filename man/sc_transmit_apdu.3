.SH "NAME"
sc_transmit_apdu \- Transmit an APDU structure
.SH "SYNOPSIS"
.PP

.sp
.RS 3n
.nf
#include <opensc.h>

int sc_transmit_apdu(struct sc_card *card, sc_apdu_t *apdu);
		
.fi
.RE
.sp
.SH "DESCRIPTION"
.PP
